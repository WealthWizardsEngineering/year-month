    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-ramda": "2.5.1",
    "jsverify": "0.8.4",
    "ramda": "0.26.1",
    "tape": "^5.0.0-next.1"
